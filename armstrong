#include <stdio.h>
#include<math.h>
int  main() 
{
    int n,rem,rev=0,original,cube,sum;
    printf("enter the numnber :");
    scanf("%d",&n);
    original=n;
    while(n!=0)
    {
        rem=n%10;
        sum=sum+pow(rem,3);
        n=n/10;
    }
    printf("the sum value is: %d\n",sum);
    if(original==sum)
    {
        printf("the given number is amstrong");
    }
else
{
    printf("the given number  is not a amstrong");
}
    return 0;
}
